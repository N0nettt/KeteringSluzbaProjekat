<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SlaniProizvodiServlet</servlet-name>
        <servlet-class>Servlets.SlaniProizvodiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SlatkiProizvodiServlet</servlet-name>
        <servlet-class>Servlets.SlatkiProizvodiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cart</servlet-name>
        <servlet-class>Servlets.Cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlacanjeServlet</servlet-name>
        <servlet-class>Servlets.PlacanjeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManagingServlet</servlet-name>
        <servlet-class>Servlets.ManagingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KorisniciServlet</servlet-name>
        <servlet-class>Servlets.KorisniciServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManadzerPanelServlet</servlet-name>
        <servlet-class>Servlets.ManadzerPanelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SlaniProizvodiServlet</servlet-name>
        <url-pattern>/slaniproizvodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SlatkiProizvodiServlet</servlet-name>
        <url-pattern>/slatkiproizvodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlacanjeServlet</servlet-name>
        <url-pattern>/placanje</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManagingServlet</servlet-name>
        <url-pattern>/managing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPanelServlet</servlet-name>
        <url-pattern>/apanel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KorisniciServlet</servlet-name>
        <url-pattern>/korisnici</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManadzerPanelServlet</servlet-name>
        <url-pattern>/mpanel</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            900
        </session-timeout>
    </session-config>
</web-app>
